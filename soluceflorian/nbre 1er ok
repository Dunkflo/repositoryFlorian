int main()
{
    int nombre=1, compteur=0;
    int i,r,n=100;
    printf("Les nombres premiers inférieurs à %d sont:\n",n);
    while(nombre <n){
         r=0;
         nombre++;
         for (i=1 ; i<=nombre ; i++)
         {
             if ((nombre%i)==0) 
                r++;
         }
         if(r==2)
         {
            printf(" %d \n",nombre);
         }
   }